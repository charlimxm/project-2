<div class="container">
  <h2>Savings Account</h2>

  <br>

  {{#each savingsAccounts as |SA|}}
    <div class>
      <div class="row col s12 m6">

        <ul class="left">

          <li><b>{{SA.name}}</b></li>

        </ul>

        <br>

      </div>

      <div class="row">

        <div class="col s12 m6">

          <ul>
            <li><img src="{{SA.bank_img}}" height="60"></li>
          </ul>

        <br>

        <ul>
          <li>Minimum Interest Rate: {{SA.min_interest_rate}}%</li>
        </ul>

        <br>

        <ul>
          <li>Maximum Interest Rate: {{SA.max_interest_rate}}%</li>
        </ul>

        <br>

        <ul>
          <li>Initial Deposit: {{SA.minimum_deposit}}</li>
        </ul>

        <br>

        <ul class="left">
          <li><a href="{{SA.link}}" class="brand-logo waves-effect waves-light btn right">Apply Here</a></li>
        </ul>

        <br>
        <br>

      </div>

      {{!-- <div class="row col s12 m6">

        <ul class="collapsible z-depth-0" data-collapsible="accordion" >

        <li>
          <div class="collapsible-header row col s12 m6"><ul class="center">

            More Details

          </ul></div>

          <div class="collapsible-body">

            <p>Minimum Age: {{SA.minimum_age}}</p>
            <p>Minimum Deposit: $ {{sa.minimum_deposit.[0]}}</p>
            <p>Fees and Charges: {{SA.fees_charges}}</p>

          </div>

        </li>

      </ul>
      </div> --}}

    </div>

    <br>

  {{/each}}

  <br>
  <br>
</div>
